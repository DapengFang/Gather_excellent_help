<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_talent_show"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.gather_excellent_help.ui.activity.shop.TalentShowActivity">

    <include layout="@layout/top_zhuangai_bar"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:lines="2"
        android:layout_centerInParent="true"
        android:textSize="16sp"
        android:textColor="#000000"
        android:text="提交成功\n我们将在1个工作日内与您联系\n查询进度电话：028-67876688"/>

    <TextView
        android:id="@+id/tv_talent_submit"
        android:layout_width="@dimen/x240"
        android:layout_height="@dimen/x40"
        android:layout_centerHorizontal="true"
        android:background="@drawable/register_confirm_selector"
        android:gravity="center"
        android:text="确定"
        android:textColor="#ffffff"
        android:textSize="@dimen/x16"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="@dimen/x80"/>

</RelativeLayout>
