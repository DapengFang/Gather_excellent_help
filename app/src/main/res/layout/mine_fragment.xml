                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/x30">

                <RelativeLayout
                    android:id="@+id/rl_mine_set"
                    android:layout_width="@dimen/x45"
                    android:layout_height="@dimen/x45"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/x20">
                    <ImageView
                        android:id="@+id/iv_me_person_set"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/icon_set"
                       android:layout_centerHorizontal="true"
                        android:layout_alignParentRight="true"/>

                </RelativeLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="个人中心"
                    android:textColor="@color/white"
                    android:layout_centerHorizontal="true"
                    android:textSize="14sp"/>
                <ImageView
                    android:id="@+id/iv_me_person_lingdang"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/x20"
                    android:src="@drawable/white_left_arraw"
                    android:visibility="visible" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_me_nickname"
                android:layout_width="@dimen/x80"
                android:layout_height="wrap_content"
                android:layout_below="@id/civ_me_head_icon"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="@dimen/x10">

                <TextView
                    android:id="@+id/tv_me_nickname"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#00000000"
                    android:gravity="center"
                    android:layout_centerInParent="true"
                    android:text="聚优帮用户"
                    android:textColor="#ffffff"
                    android:textSize="10sp" />

            </RelativeLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/rl_me_nickname"
                android:orientation="horizontal"
                android:gravity="center"
                android:layout_marginTop="@dimen/x3">
                <ImageView
                    android:layout_width="@dimen/x14"
                    android:layout_height="@dimen/x14"
                    android:src="@drawable/icon_user_type"/>
