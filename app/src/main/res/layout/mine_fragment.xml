                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="0.00"
                            android:textSize="12sp"
                            android:textColor="#55000000"/>
                    </LinearLayout>


                </LinearLayout>
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/arraw_right_jiantou"
                    android:padding="@dimen/x3"/>

            </LinearLayout>


        </LinearLayout>

